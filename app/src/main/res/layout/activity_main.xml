<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".MainActivity">
	<SeekBar
			android:layout_width="0dp"
			android:layout_height="wrap_content" android:id="@+id/sbNumbers"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="25dp"
			android:layout_marginStart="25dp" android:layout_marginEnd="25dp"/>
	<ListView
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			app:layout_constraintTop_toBottomOf="@+id/sbNumbers" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginTop="75dp" android:id="@+id/lvResults"/>
</androidx.constraintlayout.widget.ConstraintLayout>